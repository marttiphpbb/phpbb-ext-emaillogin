{
	"name": "marttiphpbb/showphpbbevents",
	"type": "phpbb-extension",
	"description": "This extension for developers shows all phpBB core template and PHP events (phpBB 3.2.2)",
	"homepage": "https://github.com/marttiphpbb/phpbb-ext-showphpbbevents",
	"version": "1.5.1",
	"time": "2018-04-15",
	"keywords": ["phpbb", "extension", "template", "PHP", "events", "development"],
	"license": "GPL-2.0",
	"authors": [{
			"name": "martti",
			"email": "phpbb@martti.be",
			"homepage": "https://github.com/marttiphpbb/",
			"role": "Developer"
		}],
	"require": {
		"php": ">=7",
		"fabpot/goutte": "^3.2"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Show phpBB Events",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0"
		},
		"version-check": {
			"host": "marttiphpbb.github.io",
			"directory": "/version",
			"filename": "showphpbbevents.json"
		}
	}
